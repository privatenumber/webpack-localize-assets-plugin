{
	"name": "webpack-localize-assets-plugin",
	"version": "0.0.0-semantic-release",
	"description": "Localize your Webpack bundle with multiple locales",
	"keywords": [
		"webpack",
		"plugin",
		"localize",
		"localization",
		"i18n",
		"internationalize",
		"internationalization",
		"multi-language"
	],
	"license": "MIT",
	"repository": "privatenumber/webpack-localize-assets-plugin",
	"funding": "https://github.com/privatenumber/webpack-localize-assets-plugin?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"types": "./dist/plugin-type.d.ts",
	"scripts": {
		"lint": "eslint .",
		"build": "pkgroll",
		"type-check": "tsc --noEmit",
		"test": "pnpm build && pnpm test:wp4 && pnpm test:wp5",
		"test:wp4": "pnpm --use-node-version=12.22.12 exec jest",
		"test:wp5": "WEBPACK=5 pnpm --use-node-version=12.22.12 exec jest"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm build && pnpm lint-staged && pnpm test"
	},
	"lint-staged": {
		"*.{js,ts}": "eslint"
	},
	"peerDependencies": {
		"webpack": "^4.42.0 || ^5.10.0"
	},
	"dependencies": {
		"@types/estree": "^0.0.50",
		"acorn": "^8.6.0",
		"astring": "^1.8.1",
		"has-own-prop": "^2.0.0",
		"magic-string": "^0.25.7",
		"source-map": "^0.7.3",
		"webpack-sources": "^2.2.0"
	},
	"devDependencies": {
		"@pvtnbr/eslint-config": "^0.33.0",
		"@types/estree": "^0.0.50",
		"@types/jest": "^27.0.3",
		"@types/mini-css-extract-plugin": "^2.4.0",
		"@types/webpack": "^4.41.32",
		"@types/webpack-manifest-plugin": "^3.0.5",
		"@types/webpack-sources": "^3.2.0",
		"css-loader": "^5.2.4",
		"es-jest": "^1.5.0",
		"eslint": "^8.28.0",
		"jest": "^27.3.1",
		"lint-staged": "^12.1.2",
		"mini-css-extract-plugin": "^1.6.0",
		"pkgroll": "^1.5.0",
		"simple-git-hooks": "^2.8.0",
		"tempy": "^1.0.1",
		"typescript": "^4.9.3",
		"webpack": "4.42.0",
		"webpack-manifest-plugin": "^3.1.1",
		"webpack-test-utils": "^1.1.0",
		"webpack5": "npm:webpack@5.10.1"
	},
	"eslintConfig": {
		"extends": "@pvtnbr"
	}
}
