{
	"name": "webpack-localize-assets-plugin",
	"version": "0.0.0-semantic-release",
	"description": "Localize your Webpack bundle with multiple locales",
	"keywords": [
		"webpack",
		"plugin",
		"localize",
		"localization",
		"i18n",
		"internationalize",
		"internationalization",
		"multi-language"
	],
	"license": "MIT",
	"repository": "privatenumber/webpack-localize-assets-plugin",
	"funding": "https://github.com/privatenumber/webpack-localize-assets-plugin?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"scripts": {
		"lint": "eslint .",
		"build": "tsc",
		"typecheck": "tsc --noEmit",
		"test": "npm run test:wp4 && npm run test:wp5",
		"test:wp4": "jest",
		"test:wp5": "WEBPACK=5 jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run build && lint-staged && npm test"
		}
	},
	"lint-staged": {
		"*.{js,ts}": "eslint"
	},
	"dependencies": {
		"@types/estree": "^0.0.50",
		"acorn": "^8.5.0",
		"astring": "^1.7.5",
		"has-own-prop": "^2.0.0",
		"magic-string": "^0.25.7",
		"webpack-sources": "^2.2.0"
	},
	"devDependencies": {
		"@pvtnbr/eslint-config": "^0.5.0",
		"@types/estree": "^0.0.50",
		"@types/jest": "^26.0.24",
		"@types/mini-css-extract-plugin": "^2.4.0",
		"@types/webpack": "^4.41.31",
		"@types/webpack-manifest-plugin": "^3.0.5",
		"aggregate-error": "^4.0.0",
		"css-loader": "^5.2.4",
		"es-jest": "^1.2.0",
		"eslint": "^7.32.0",
		"esno": "^0.5.0",
		"fs-require": "^1.0.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"memfs": "^3.2.2",
		"mini-css-extract-plugin": "^1.6.0",
		"source-map": "^0.7.3",
		"tempy": "^1.0.1",
		"typescript": "^4.4.4",
		"unionfs": "^4.4.0",
		"webpack": "4.42.0",
		"webpack-manifest-plugin": "^3.1.1",
		"webpack-test-utils": "^1.1.0",
		"webpack5": "npm:webpack@5.10.1"
	},
	"eslintConfig": {
		"extends": "@pvtnbr"
	}
}
